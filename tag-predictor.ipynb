    "    print(\"Jacard score: {}\".format(avg_jacard(tags_test, y_pred)))\n",
    "    print(\"Hamming loss: {}\".format(hamming_loss(y_pred, tags_test)*100))\n",
    "    print(\"---\")"
   ]
